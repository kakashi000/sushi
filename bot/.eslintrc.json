{
    "env": {
        "commonjs": true,
        "es6": true
    },
    "extends": "airbnb-base",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "no-console": 0,
        "prefer-destructuring": 0,
        "consistent-return": 0,
        "no-restricted-syntax": 0,
        "no-loop-func": 0
    }
}